{
    "name": "silverstripe/docblock-writer",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\DocblockWriter\\": "src/",
            "SilverStripe\\DocblockWriter\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "phpcs code/ tests/php/",
        "lint-clean": "phpcbf code/ tests/php/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
